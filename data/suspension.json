        {"name": "F_SprungWeight", "relationship": "F_Weight - F_UnsprungWeight"},
        {"name": "R_SprungWeight", "relationship": "R_Weight - R_UnsprungWeight"},
        {"name": "SprungWeight", "relationship": "F_SprungWeight + R_SprungWeight"},
        {"name": "F_UnsprungWeightTfr", "relationship": "( F_UnsprungWeight * F_COGHeight ) / F_TrackWidth"},
        {"name": "R_UnsprungWeightTfr", "relationship": "( R_UnsprungWeight * R_COGHeight ) / R_TrackWidth"},
        {"name": "F_RollCenterWeightTfr", "relationship": "( F_SprungWeight * F_RollCenterHeight ) / F_TrackWidth"},
        {"name": "R_RollCenterWeightTfr", "relationship": "( R_SprungWeight * R_RollCenterHeight ) / R_TrackWidth"},
        {"name": "F_WeightDistribution", "relationship": "F_SprungWeight / SprungWeight"},
        {"name": "R_WeightDistribution", "relationship": "one - F_WeightDistribution"},
        {"name": "MeanCOGHeight", "relationship": "( ( R_COGHeight - F_COGHeight ) * R_WeightDistribution ) + F_COGHeight"},
        {"name": "MeanRollCenter", "relationship": "( ( R_RollCenterHeight - F_RollCenterHeight ) * R_WeightDistribution ) + F_RollCenterHeight"},
        {"name": "MeanTrack", "relationship": "( ( R_TrackWidth - F_TrackWidth ) * R_WeightDistribution ) + F_TrackWidth"},
        {"name": "RollCenterCOGLeverArm", "relationship": "MeanCOGHeight - MeanRollCenter"},
        {"name": "SprungTransfer", "relationship": "(SprungWeight * RollCenterCOGLeverArm) / MeanTrack"},
        {"name": "F_WeightTransfer", "relationship": "F_UnsprungWeightTfr + F_RollCenterWeightTfr"},
        {"name": "R_WeightTransfer", "relationship": "R_UnsprungWeightTfr + R_RollCenterWeightTfr"},
        {"name": "WeightTransfer", "relationship": "F_WeightTransfer + R_WeightTransfer + SprungTransfer"},
        {"name": "RollStiffness", "relationship": "(pi * F_SpringRate * F_TrackWidth ^2) / ((F_WheelMovement/F_SpringMovement)^2 * 360)"},
        {"name": "RollbarAngularStiffness", "relationship": "RollBarShear * RollBarJ / RollBarLength"}
